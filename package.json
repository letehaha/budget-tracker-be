{
  "name": "budget-tracker-be",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development nodemon",
    "prod": "NODE_ENV=production nodemon",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-undo": "npx sequelize-cli db:seed:undo:all",
    "test": "npm run test:unit",
    "test:unit": "jest --passWithNoTests",
    "lint": "eslint .",
    "docker-build": "docker build . -t letehaha/budget-tracker-be",
    "docker-push": "docker push letehaha/budget-tracker-be",
    "docker-compose": "npm run docker-build && npm run docker-push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letehaha/budget-tracker-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/letehaha/budget-tracker-be/issues"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-commit": "npm run lint"
    }
  },
  "_moduleAliases": {
    "@/src/": "build/src/",
    "@js": "src/js",
    "@routes": "src/routes",
    "@middlewares": "src/middlewares",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@services": "src/services",
    "shared-types": "shared-types"
  },
  "homepage": "https://github.com/letehaha/budget-tracker-be#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "currency-codes": "^2.1.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.2",
    "express-validator": "^6.6.1",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "locale": "^0.1.0",
    "morgan": "^1.10.0",
    "p-queue": "^6.6.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.12.2",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "uuid": "^9.0.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/locale": "^0.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.4",
    "@types/passport": "^1.0.7",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^28.1.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.6",
    "ts-jest": "^28.0.4",
    "tslint": "^6.1.3"
  }
